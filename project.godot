config_version=4

[application]

config/name="UNC_Niner_Rides"
run/main_scene="res://Scenes/HomeScreen.tscn"
config/icon="res://Assets/icon.png"

[display]

window/size/width=360
window/size/height=640

[physics]

common/enable_pause_aware_picking=true

[rendering]

